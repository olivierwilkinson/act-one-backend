{
  "name": "act-one-backend",
  "version": "1.3.0",
  "description": "ActOne express backend",
  "author": "Olivier Wilkinson",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "build": "nexus build",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "dev": "NODE_ENV=development nodemon",
    "dev:local": "./scripts/dev-local.sh",
    "start": "node dist",
    "test": "jest",
    "fix-lint": "prettier --write src && ./node_modules/eslint/bin/eslint.js  src --ext .ts --fix",
    "migrate-save": "prisma migrate save --experimental",
    "migrate-up": "prisma migrate up --experimental",
    "migrate": "npm run migrate-save && npm run migrate-up",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "NODE_ENV=development ts-node --transpile-only src/schema"
  },
  "dependencies": {
    "@nexus/schema": "^0.18.0",
    "@prisma/client": "^2.11.0",
    "@quixo3/prisma-session-store": "^1.1.0",
    "@sentry/node": "^5.20.1",
    "apollo-server-express": "^2.19.0",
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.4.0",
    "http-proxy-middleware": "^1.0.5",
    "nexus-plugin-prisma": "^0.24.0",
    "nodemon": "2.0.4",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "strong-error-handler": "^3.5.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.11.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "eslint": "7.3.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.1.0",
    "jest-express": "1.12.0",
    "prettier": "^2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "3.9.5"
  }
}
